{
    "description": "THis is the desc",
    "method": "GET",
    "endpoint": "/wow/endpoint/read/from/file",
    "security": [
        {
            "type": "Bearer token",
            "name": "Authorization",
            "value": "Bearer <token>",
            "in": "headers"
        },
        {
            "type": "API key",
            "name": "api_key",
            "value": "<api_key>",
            "in": "query"
        }
    ],
    "parameters": [
        {
            "name": "id",
            "description": "The ID of the folder to retrieve",
            "required": true,
            "type": "string",
            "in": "body"
        }
    ],
    "responses": [
        {
            "status": 200,
            "description": "The folder was found",
            "schema": [
                {
                    "name": "id",
                    "type": "string",
                    "description": "The ID of the folder"
                },
                {
                    "name": "name",
                    "type": "string",
                    "description": "The name of the folder"
                }
            ]
        },
        {
            "status": 400,
            "description": "Bad request"
        },
        {
            "status": 401,
            "description": "Unauthorized"
        }
    ]
}